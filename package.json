  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.7.1",
    "@types/node": "^20.0.0",
    "eslint": "^9.39.2",
    "rimraf": "^6.1.2",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.54.0"
  },
  "openclaw": {
    "extensions": [
      "./build/index.js"
