{
  "name": "claw-hass",
  "version": "1.0.1",
  "description": "OpenClaw Home Assistant Integration",
  "main": "build/index.js",
  "type": "module",
  "keywords": [
    "clawdbot",
    "openclaw",
    "plugin",
    "automation"
  ],
  "author": "Tobias Strebitzer <tobias.strebitzer@gmail.com>",
  "license": "MIT",
